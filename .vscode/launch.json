{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "io_bench",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/bin/iobench",
      "args": [
        "4M", "20M", "sequential", "0", "cufile_batch", "test_file", "4"
      ],
      "stopAtEntry": true,
      "justMyCode": false,
      "cwd": "${workspaceFolder}",
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}